const patterns = [
  { emotion: 'sadness', words: ['ุญุฒูู', 'ุถุงูู', 'ูุฆูุจ', 'ุฏููุน'] },
  { emotion: 'happiness', words: ['ุณุนูุฏ', 'ูุฑุญุงู', 'ูุจุณูุท'] },
  { emotion: 'love', words: ['ุงุญุจ', 'ุญุจ', 'ุงุนุดู'] },
  { emotion: 'fear', words: ['ุฎุงุฆู', 'ููู', 'ูุฑุนูุจ'] },
  { emotion: 'anger', words: ['ุบุงุถุจ', 'ูุนุตุจ', 'ููููุฑ'] },
  { emotion: 'loneliness', words: ['ูุญูุฏ', 'ูุญุฏุฉ', 'ููุญุฏู'] },
  { emotion: 'stress', words: ['ุชุนุจุงู', 'ูุฑูู', 'ูุถุบูุท'] }
];

function getEmotionReply(text) {
  for (let p of patterns) {
    for (let w of p.words) {
      if (text.includes(w)) {
        const responses = emotionalResponses[p.emotion];
        return responses[Math.floor(Math.random() * responses.length)];
      }
    }
  }
  return "ุฃูุง ููุง ูุฃุณุชูุน ุฅูููุ ุชุญุฏุซ ุฅููู ุจูู ูุง ูู ููุจู.";
}

function sendMessage() {
  const input = document.getElementById("input");
  const text = input.value.trim();
  if (text === "") return;

  addMessage("ุฃูุช", text, "user");
  const reply = getEmotionReply(text.toLowerCase());
  setTimeout(() => addMessage("ุงููุณุงุนุฏ", reply, "bot"), 500);
  input.value = "";
}

function addMessage(sender, text, className) {
  const chat = document.getElementById("chat");
  const msg = document.createElement("div");
  msg.className = className;
  msg.textContent = `${sender}: ${text}`;
  chat.appendChild(msg);
  chat.scrollTop = chat.scrollHeight;
}
</script>

</body>
</html>
